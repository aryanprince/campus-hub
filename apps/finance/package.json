{
  "name": "@campus-hub/finance",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "dev": "pnpm with-env next dev --port 3003",
    "start": "pnpm with-env next start",
    "clean": "git clean -xdf .next .turbo node_modules",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv --",
    "with-global-env": "dotenv -e ../../.env --",
    "db:studio": "drizzle-kit studio --port 3013",
    "db:push": "drizzle-kit push:pg",
    "db:migrate:generate": "drizzle-kit generate:pg",
    "db:migrate:run": "pnpm with-env tsx ./src/server/db/migrate.ts",
    "db:migrate:push": "pnpm db:migrate:generate && db:migrate:run",
    "db:seed": "pnpm with-env tsx ./src/server/db/seed.ts",
    "db:setup": "pnpm db:migrate:run && pnpm db:seed"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.6",
    "@campus-hub/ui": "workspace:^0.1.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.32.0",
    "geist": "^1.2.1",
    "lucide-react": "^0.321.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "postgres": "^3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sonner": "^1.4.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@campus-hub/eslint-config": "workspace:^0.2.0",
    "@campus-hub/prettier-config": "workspace:^0.1.0",
    "@campus-hub/tailwind-config": "workspace:^0.1.0",
    "@campus-hub/tsconfig": "workspace:^0.1.0",
    "@types/node": "^18.19.14",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.17",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.23.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@campus-hub/eslint-config/base",
      "@campus-hub/eslint-config/nextjs",
      "@campus-hub/eslint-config/react"
    ]
  },
  "prettier": "@campus-hub/prettier-config",
  "ct3aMetadata": {
    "initVersion": "7.26.0"
  }
}
