{
  "name": "@campus-hub/finance",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.9.2",
  "scripts": {
    "build": "pnpm with-env next build",
    "dev": "pnpm with-env next dev --port 3003",
    "start": "pnpm with-env next start",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "clean": "git clean -xdf .next .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "db:studio": "drizzle-kit studio",
    "db:direct-push": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx --env-file .env ./src/server/db/migrate.ts",
    "db:push": "pnpm run db:generate && pnpm run db:migrate",
    "db:seed": "tsx --env-file .env ./src/server/db/seed.ts",
    "db:reset": "pnpm run db:migrate && pnpm run db:seed"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.6",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "drizzle-orm": "^0.29.3",
    "geist": "^1.2.1",
    "lucide-react": "^0.321.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "postgres": "^3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@campus-hub/eslint-config": "workspace:^0.2.0",
    "@campus-hub/prettier-config": "workspace:^0.1.0",
    "@campus-hub/tailwind-config": "workspace:^0.1.0",
    "@campus-hub/tsconfig": "workspace:^0.1.0",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.9",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/nextjs",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config",
  "ct3aMetadata": {
    "initVersion": "7.26.0"
  }
}
