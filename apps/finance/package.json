    "geist": "^1.2.1",
    "lucide-react": "^0.321.0",
    "next": "^14.0.4",
    "next-themes": "^0.2.1",
    "postgres": "^3.4.3",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "server-only": "^0.0.1",
    "sonner": "^1.4.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.0",
    "zod": "^3.22.4"
